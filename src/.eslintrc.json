{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "arrow-parens": "off",
    "no-plusplus": "off",
    "jsx-quotes": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "react/prop-types": "off",
    "arrow-body-style": "off",
    "no-noninteractive-element-interactions": "off",
    "no-confusing-arrow": "off"
  }
}
